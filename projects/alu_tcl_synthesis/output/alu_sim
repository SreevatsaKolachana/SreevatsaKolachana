 .timescale 0 0;
S_0x5d881078a680 .scope module, "alu_tb" "alu_tb" 3 3;
 .timescale -9 -12;
v0x5d88107e92a0_0 .var "a", 3 0;
v0x5d88107e9380_0 .var "b", 3 0;
