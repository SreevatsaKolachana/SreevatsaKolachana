    .port_info 5 /INPUT 3 "opcode";
    .port_info 6 /OUTPUT 4 "result";
    .port_info 7 /OUTPUT 1 "done";
P_0x5d88107e8490 .param/l "DONE" 0 4 16, C4<10>;
P_0x5d88107e84d0 .param/l "EXECUTE" 0 4 16, C4<01>;
