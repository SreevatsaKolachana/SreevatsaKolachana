E_0x5d88107c9c50 .event posedge, v0x5d88107e8ad0_0;
E_0x5d88107c9ea0 .event anyedge, v0x5d88107e8b90_0;
E_0x5d88107cbb10 .event negedge, v0x5d88107e8ad0_0;
TD_alu_tb.apply_inputs ;
    %wait E_0x5d88107cbb10;
    %load/vec4 v0x5d88107b2860_0;
    %store/vec4 v0x5d88107e95f0_0, 0, 3;
    %load/vec4 v0x5d88107b45f0_0;
    %store/vec4 v0x5d88107e92a0_0, 0, 4;
    %load/vec4 v0x5d88107b4690_0;
    %store/vec4 v0x5d88107e9380_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d88107e9830_0, 0, 1;
    %wait E_0x5d88107cbb10;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d88107e9830_0, 0, 1;
T_0.0 ;
    %load/vec4 v0x5d88107e9520_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.1, 6;
    %wait E_0x5d88107c9ea0;
    %jmp T_0.0;
T_0.1 ;
    %wait E_0x5d88107c9c50;
    %vpi_call/w 3 72 "$display", "Opcode: %0d, A: %0d, B: %0d => Result: %0d, Done: %0d", v0x5d88107e95f0_0, v0x5d88107e92a0_0, v0x5d88107e9380_0, v0x5d88107e9690_0, v0x5d88107e9520_0 {0 0 0};
    %end;
S_0x5d88107e8290 .scope module, "dut" "alu_fsm" 3 17, 4 5 0, S_0x5d881078a680;
